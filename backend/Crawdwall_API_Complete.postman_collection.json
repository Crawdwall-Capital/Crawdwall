    {
      "name": "2. Admin OTP Authentication",
      "description": "OTP-based authentication system for admin users",
      "item": [
        {
          "name": "Request Admin OTP",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/auth/admin/request-otp",
              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "auth",
                "admin",
                "request-otp"
              ]
            },
            "description": "Super admin requests OTP to be sent to another admin email.",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"superAdminEmail\": \"thiscrawdwallcapital@gmail.com\",\n  \"adminEmail\": \"{{adminEmail}}\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          },
          "response": [
            {
              "name": "Success Response",
              "status": "Created",
              "code": 201,
              "_postman_previewlanguage": "json",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n  \"message\": \"OTP sent successfully to admin email\",\n  \"adminEmail\": \"admin@example.com\"\n}"
            }
          ]
        },
        {
          "name": "Verify Admin OTP",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "url": {
              "raw": "{{baseUrl}}/auth/admin/verify-otp",
