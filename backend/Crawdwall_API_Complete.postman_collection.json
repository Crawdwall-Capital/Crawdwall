              "host": [
                "{{baseUrl}}"
              ],
              "path": [
                "organizer",
                "proposals"
              ]
            },
            "description": "Submit a new event proposal. Only for ORGANIZER role.",
            "auth": {
              "type": "bearer",
              "bearer": [
                {
                  "key": "token",
                  "value": "{{authToken}}",
