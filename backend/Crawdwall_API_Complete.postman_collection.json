            "description": "Super admin adds a new admin user to the system.",
            "body": {
              "mode": "raw",
              "raw": "{\n  \"superAdminEmail\": \"thiscrawdwallcapital@gmail.com\",\n  \"adminEmail\": \"newadmin@example.com\",\n  \"adminName\": \"New Admin\"\n}",
              "options": {
                "raw": {
                  "language": "json"
                }
              }
            }
          },
          "response": [
            {
              "name": "Success Response",
              "status": "Created",
              "code": 201,
              "_postman_previewlanguage": "json",
              "header": [
                {
                  "key": "Content-Type",
                  "value": "application/json"
                }
              ],
              "body": "{\n  \"success\": true,\n  \"message\": \"Admin email newadmin@example.com authorized successfully\",\n  \"admin\": {\n    \"id\": \"uuid\",\n    \"email\": \"newadmin@example.com\",\n    \"name\": \"New Admin\",\n    \"status\": \"ACTIVE\",\n    \"createdBy\": \"thiscrawdwallcapital@gmail.com\"\n  }\n}"
            }
          ]
        }
      ]
